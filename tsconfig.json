{
  "compilerOptions": {
    "target": "ESNext",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "isolatedModules": true,
    "resolveJsonModule": true,
    "baseUrl": "src",
    "allowJs": true,
    "checkJs": true,
    "strict": false,
    "jsx": "react-jsx",
    "skipLibCheck": true
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
